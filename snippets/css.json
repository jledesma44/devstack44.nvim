    "description": "json media query landscape snippet"
  },
  "Media-query-max-height": {
    "prefix": "mqmaxh",
    "body": [
      "",
      "@media (max-height: ${1:500px}) {",
      "  ${2:selector} {",
      "  }",
      "}    "
    ],
    "description": "json media query max-height snippet"
  },
  "Media-query-min-max": {
    "prefix": "mqmin-max",
    "body": [
      "",
      "@media (min-width: ${1:500px}) and (max-width ${2:500px}) {",
      "  ${3:selector} {",
      "  }",
      "}    "
    ],
    "description": "json media query min-max snippet"
  },
  "Media-query-container-max-width": {
    "prefix": "mqcmax",
    "body": [
      "",
      "@container ${1:aside}(max-width: ${2:900px}) {",
      "  ${3:selector} {",
      "  }",
      "}    "
    ],
    "description": "json query container max-width snippet"
  },
  "Google font": {
    "prefix": "gfonts",
    "body": [
      "@import url('https://fonts.googleapis.com/css2?family=${1:Fraunces}wght@200,300,400,500,600,700,800,900&family=${4:Manrope}wght@200,300,400,500,600,700,800,900&display=swap');"
    ],
    "description": "json google fonts snippet"
  },
  "spacing-reset": {
    "prefix": "spacing-reset",
    "body": [
      "* {",
      "  padding: 0;",
      "  margin: 0;",
      "  box-sizing: border-box;",
      "}"
    ],
    "description": "json spacing reset snippet"
  },
  "box-shadow": {
    "prefix": "box-shadow",
    "body": [
      "${1:.card} {",
      "  box-shadow: ${2:0}px ${3:10}px ${4:40}px ${5:0}px rgba(0, 0, 0, ${6:0.15});$0",
      "} ",
      " ",
      ""
    ],
    "description": "json box shadow snippet"
  },
  "transparent button": {
    "prefix": "btn-trans",
    "body": [
      ".btn {",
      "  padding: ${1:12}px ${2:22}px;",
      "  border-radius: ${3:15}px;",
      "  border: 1px solid var(${4:--medium-gray});$0",
      "} ",
      " ",
      ""
    ],
    "description": "json btn transparent snippet"
  },
  "solid button": {
    "prefix": "btn-solid",
    "body": [
      ".btn {",
      "  padding: ${1:12}px ${2:22}px;",
      "  border-radius: ${3:15}px;",
      "  background: ${4:steelblue};",
      "  color: ${5:white};$0",
      "} ",
      " ",
      ""
    ],
    "description": "json btn solid snippet"
  },
  "linear gradient": {
    "prefix": "linear-grad",
    "body": [
      ".card {",
      "  background-image: linear-gradient(to right, ${1:#ed7cda}, ${2:#ffaa1b});",
      "  color: white;",
      "}",
      ""
    ],
    "description": "json linear gradient snippet"
  }
}
